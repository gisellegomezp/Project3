    "# Process the prediction\n",
    "predictions = tf.nn.softmax(logits, axis=-1)\n",
    "predicted_class = tf.argmax(predictions, axis=-1).numpy()\n",
    "\n",
    "print(f\"Predicted class: {predicted_class[0]}\")\n"
   ]
