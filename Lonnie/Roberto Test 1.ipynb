    "from transformers import RobertaTokenizer, TFRobertaForSequenceClassification\n",
    "import tensorflow as tf\n",
    "\n",
    "# Load tokenizer and model\n",
    "tokenizer = RobertaTokenizer.from_pretrained('roberta-base')\n",
